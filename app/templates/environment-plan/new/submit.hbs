{{#nop-page title='Review Your Submission' class='submit-page'}}
  <section>
    {{nop-activity-description submission=model disabled=true}}
  </section>
  <section>
    {{nop-titles submission=model disabled=true}}
  </section>
  <section>
    {{nop-activity-types submission=model disabled=true}}
  </section>
  <section>
    {{nop-titleholders submission=model disabled=true}}
  </section>
  {{#each errorMessages as |message|}}
    <div>{{message}}</div>
  {{/each}}
  {{#nop-actions}}
    <button {{action 'validate'}}>Validate</button>
    <button {{action 'goBack'}}>Back</button>
    <button class='primary' {{action 'submit'}}>Submit</button>
  {{/nop-actions}}
{{/nop-page}}
